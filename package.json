    "sso",
    "single-sign-on",
    "oauth",
    "jwt",
    "2fa",
    "two-factor-auth",
    "mfa",
    "multi-factor-auth",
    "user-management",
    "user-types",
    "organization",
    "organization-types",
    "multi-tenant",
    "tenancy",
    "billing",
    "billing-types",
    "subscription",
    "subscriptions",
    "pricing",
    "stripe",
    "payment",
    "invoice",
    "product",
    "product-types",
    "feature-flags",
    "feature-management",
    "email-service",
    "email-types",
    "file-service",
    "file-upload",
    "storage",
    "search-service",
    "database-types",
    "postgres",
    "mongodb",
    "validation",
    "validators",
    "zod",
    "schema-validation",
    "runtime-validation",
    "type-safety",
    "type-safe",
    "strongly-typed",
    "backend-types",
    "frontend-types",
    "fullstack",
    "nodejs",
    "node",
    "express",
    "nestjs",
    "nextjs",
    "react",
    "api-contracts",
    "dto",
    "data-transfer-object",
    "domain-model",
    "entities",
    "repository",
    "tree-shakeable",
    "esm",
    "commonjs",
    "modular",
    "microservices",
    "monorepo",
    "production-ready",
    "kitium",
    "kitiumai"
  ],
  "author": {
    "name": "Kitium AI"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitium-ai/types.git"
  },
  "homepage": "https://github.com/kitium-ai/types#readme",
  "bugs": {
    "url": "https://github.com/kitium-ai/types/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@kitiumai/config": "^2.0.1",
    "@kitiumai/lint": "^2.0.1",
    "@kitiumai/scripts": "^1.0.0",
    "@kitiumai/vitest-helpers": "^2.0.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "graphql": "^16.12.0",
    "typescript": "^5.6.0",
    "tsd": "^0.31.0",
    "tsup": "^8.2.4",
    "vitest": "^2.1.3"
  }
